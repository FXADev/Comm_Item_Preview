# Redshift connection parameters
REDSHIFT_HOST=your_redshift_host
REDSHIFT_PORT=5439
REDSHIFT_USER=your_username
REDSHIFT_PASSWORD=your_password
REDSHIFT_DATABASE=your_database

# Salesforce connection parameters
SF_USERNAME=your_sf_username
SF_PASSWORD=your_sf_password
SF_SECURITY_TOKEN=your_sf_security_token

# SQL Server connection parameters for staging tables
SQL_SERVER=your_sql_server
SQL_DATABASE=your_database
SQL_USERNAME=your_username
SQL_PASSWORD=your_password
SQL_DRIVER={ODBC Driver 18 for SQL Server}

# Note: For GitHub Actions workflow, these environment variables are stored as GitHub secrets
# See .github/workflows/nightly_etl.yml for how they are used
